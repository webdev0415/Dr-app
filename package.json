{
  "name": "provider-app",
  "version": "0.0.1",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "serve:local": "node --max-old-space-size=4096 ./node_modules/@angular/cli/bin/ng serve -c local",
    "serve:dev": "node --max-old-space-size=4096 ./node_modules/@angular/cli/bin/ng serve -c development",
    "serve:pilot": "node --max-old-space-size=4096 ./node_modules/@angular/cli/bin/ng serve -c pilot",
    "serve:prod": "node --max-old-space-size=4096 ./node_modules/@angular/cli/bin/ng serve --prod",
    "serve:proxy": "node --max-old-space-size=4096 ./node_modules/@angular/cli/bin/ng serve -c proxy --host=0.0.0.0",
    "build:staging": "ng build -c development && tar cJf deploy/DoctorApp.tar.xz -C dist DoctorApp",
    "build:dev": "ng build -c development && tar cJf deploy/DoctorApp.tar.xz -C dist DoctorApp",
    "build:redesign": "ng build -c developmentPE && tar cJf deploy/DoctorApp2.tar.xz -C dist DoctorApp2",
    "build:pilot": "ng build -c pilot && tar cJf deploy/DoctorApp.tar.xz -C dist DoctorApp",
    "build:prod": "ng build -c production && tar cJf deploy/DoctorApp.tar.xz -C dist DoctorApp",
    "build": "ng build -c production && tar cJf deploy/DoctorApp.tar.xz -C dist DoctorApp",
    "deploy:staging": "deploy/devdeploy.sh --prod && rimraf dist",
    "deploy:redesign": "deploy/redesign.sh --prod && rimraf dist",
    "deploy:pilot": "deploy/pilotdeploy.sh --prod && rimraf dist",
    "deploy": "deploy/proddeploy.sh --prod && rimraf dist",
    "version:up": "TS_NODE_COMPILER_OPTIONS='{\"module\":\"commonjs\"}' ts-node versioning/git.version.ts",
    "version:tag": "ts-node versioning/git.version-tag.ts",
    "test": "ng test",
    "test:local": "node --version && TESTLOCAL=true node --max-old-space-size=4096 ./node_modules/@angular/cli/bin/ng run provider-app:test:local",
    "lint": "ng lint",
    "e2e": "node e2e/mock/server.js & ng e2e",
    "e2e:serve": "node e2e/mock/server.js & ng serve --proxy-config e2e/mock/proxy.config.json -c mock",
    "e2e:no-serve": "ng e2e -c no-serve",
    "build:pharmacist": "./node_modules/@angular/cli/bin/ng build pharmacist && tar cJf libs/pharmacist.tar.gz -C dist pharmacist/",
    "build:documents-generator": "./node_modules/@angular/cli/bin/ng build documents-generator && tar cJf libs/documents-generator.tar.gz -C dist documents-generator/"
  },
